   "source": [
    "class Solution:\n",
    "    def restoreString(self, s: str, indices: List[int]) -> str:\n",
    "        result = \"\"\n",
    "        for i in range(len(indices)):\n",
    "            result += s[indices.index(i)]\n",
    "        return result"
   ]
