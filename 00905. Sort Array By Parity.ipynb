   "source": [
    "class Solution:\n",
    "    def sortArrayByParity(self, nums: List[int]) -> List[int]:\n",
    "        newNums = []\n",
    "        for i in nums:\n",
    "            if i%2 == 0:\n",
    "                newNums.insert(0, i)\n",
    "            else:\n",
    "                newNums.append(i)\n",
    "        return newNums"
   ]
  }
