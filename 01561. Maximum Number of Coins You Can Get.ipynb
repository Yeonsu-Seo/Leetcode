   "source": [
    "class Solution:\n",
    "    def maxCoins(self, piles: List[int]) -> int:\n",
    "        piles.sort(reverse=True)\n",
    "        result = 0\n",
    "        for i in range(len(piles)//3):\n",
    "            result += piles[1+i*2]\n",
    "        return result"
   ]
  }
