   "source": [
    "class Solution:\n",
    "    def distinctAverages(self, nums: List[int]) -> int:\n",
    "        avg = []\n",
    "        nums.sort()\n",
    "        while nums:\n",
    "            a = nums.pop(-1)\n",
    "            b = nums.pop(0)\n",
    "            avg.append((a+b)/2)\n",
    "        return len(set(avg))"
   ]
  }
