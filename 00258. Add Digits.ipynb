    "class Solution:\n",
    "    def addDigits(self, num: int) -> int:\n",
    "        while len(str(num)) != 1:\n",
    "            newNum = 0\n",
    "            for i in str(num):\n",
    "                newNum += int(i)\n",
    "            num = newNum\n",
    "        return num"
   ]
