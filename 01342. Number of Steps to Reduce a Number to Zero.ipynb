   "source": [
    "class Solution:\n",
    "    def numberOfSteps(self, num: int) -> int:\n",
    "        count = 0\n",
    "        while num != 0:\n",
    "            if num%2==1:\n",
    "                num -= 1\n",
    "            else:\n",
    "                num//=2\n",
    "            count += 1\n",
    "        return count"
   ]
  }
