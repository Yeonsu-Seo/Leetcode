   "source": [
    "class Solution:\n",
    "    def finalValueAfterOperations(self, operations: List[str]) -> int:\n",
    "        result = 0\n",
    "        for i in operations:\n",
    "            if \"-\" in i:\n",
    "                result -= 1\n",
    "            else:\n",
    "                result += 1\n",
    "        return result"
   ]
  }
