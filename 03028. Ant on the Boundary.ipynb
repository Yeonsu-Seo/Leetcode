   "source": [
    "class Solution:\n",
    "    def returnToBoundaryCount(self, nums: List[int]) -> int:\n",
    "        result = 0\n",
    "        check = 0\n",
    "        for i in nums:\n",
    "            check += i\n",
    "            if check == 0:\n",
    "                result += 1\n",
    "        \n",
    "        return result"
   ]
  }
