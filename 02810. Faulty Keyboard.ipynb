   "source": [
    "class Solution:\n",
    "    def finalString(self, s: str) -> str:\n",
    "        result = \"\"\n",
    "        for i in range(len(s)):\n",
    "            if s[i] != \"i\":\n",
    "                result += s[i]\n",
    "            else:\n",
    "                result = result[::-1]\n",
    "        return result"
   ]
  }
