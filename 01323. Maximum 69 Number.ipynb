   "source": [
    "class Solution:\n",
    "    def maximum69Number (self, num: int) -> int:\n",
    "        if \"6\" not in str(num):\n",
    "            return num\n",
    "        else:\n",
    "            numList = list(str(num))\n",
    "            numList[numList.index(\"6\")] = \"9\"\n",
    "            return int(\"\".join(numList))"
   ]
  }
