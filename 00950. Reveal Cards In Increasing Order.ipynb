   "source": [
    "class Solution:\n",
    "    def deckRevealedIncreasing(self, deck: List[int]) -> List[int]:\n",
    "        deck.sort(reverse=True)\n",
    "        result = [deck[0]]\n",
    "        for i in range(1, len(deck)):\n",
    "            result = [deck[i], result[-1]] + result[:len(result)-1]\n",
    "\n",
    "        return result\n",
    "        "
   ]
  }
