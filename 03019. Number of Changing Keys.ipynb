   "source": [
    "class Solution:\n",
    "    def countKeyChanges(self, s: str) -> int:\n",
    "        s= s.lower()\n",
    "        s = list(s)\n",
    "        count = 0\n",
    "        for i in range(len(s)-1):\n",
    "            if s[i] != s[i+1]:\n",
    "                count +=1\n",
    "        return count"
   ]
  }
