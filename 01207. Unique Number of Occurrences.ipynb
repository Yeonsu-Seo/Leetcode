   "source": [
    "class Solution:\n",
    "    def uniqueOccurrences(self, arr: List[int]) -> bool:\n",
    "        occurrence = []\n",
    "        for i in set(arr):\n",
    "            oc = arr.count(i)\n",
    "            if oc in occurrence:\n",
    "                return False\n",
    "            occurrence.append(oc)\n",
    "        return True"
   ]
  }
