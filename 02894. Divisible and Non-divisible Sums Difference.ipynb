    "class Solution:\n",
    "    def differenceOfSums(self, n: int, m: int) -> int:\n",
    "        count = 0\n",
    "        for i in range(1, n+1):\n",
    "            if i%m==0:\n",
    "                count -= i\n",
    "            else:\n",
    "                count += i\n",
    "        return count"
   ]
  }
