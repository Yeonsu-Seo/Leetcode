   "source": [
    "class Solution:\n",
    "    def reverseStr(self, s: str, k: int) -> str:\n",
    "        sList = list(s)\n",
    "\n",
    "        for i in range(0, len(s), 2*k):\n",
    "            sList[i:i+k] = sList[i:i+k][::-1]\n",
    "        return \"\".join(sList)"
   ]
  }
