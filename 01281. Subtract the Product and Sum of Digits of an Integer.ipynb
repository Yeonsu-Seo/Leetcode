   "source": [
    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        result = 0\n",
    "        pro = 1\n",
    "        for i in str(n):\n",
    "            result -= int(i)\n",
    "            pro *= int(i)\n",
    "        return pro+result"
   ]
  }
