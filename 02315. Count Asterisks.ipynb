   "source": [
    "class Solution:\n",
    "    def countAsterisks(self, s: str) -> int:\n",
    "        s = s.split('|')\n",
    "        result = 0\n",
    "        for i in range(len(s)):\n",
    "            if i%2==0:\n",
    "                result += s[i].count(\"*\")\n",
    "        return result\n"
   ]
  }
