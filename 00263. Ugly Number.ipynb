   "source": [
    "class Solution:\n",
    "    def isUgly(self, n: int) -> bool:\n",
    "        if n == 0:\n",
    "            return False\n",
    "        \n",
    "        for i in [2, 3, 5]:\n",
    "            while n%i==0:\n",
    "                n //= i\n",
    "        if n == 1:\n",
    "            return True\n",
    "        return False"
   ]
  }
