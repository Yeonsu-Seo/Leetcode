   "source": [
    "class Solution:\n",
    "    def squareIsWhite(self, coordinates: str) -> bool:\n",
    "        row = ord(coordinates[0])\n",
    "        col = int(coordinates[1])\n",
    "\n",
    "        if (row+col)%2==0:\n",
    "            return False\n",
    "        else:\n",
    "            return True"
   ]
  }
