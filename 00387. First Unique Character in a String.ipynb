   "source": [
    "class Solution:\n",
    "    def firstUniqChar(self, s: str) -> int:\n",
    "        sDict = {}\n",
    "        for i in s:\n",
    "            sDict[i] = sDict.get(i, 0) + 1\n",
    "        \n",
    "        for i in range(len(s)):\n",
    "            if sDict[s[i]] == 1:\n",
    "                return i\n",
    "        return -1"
   ]
  }
