    "class Solution:\n",
    "    def titleToNumber(self, columnTitle: str) -> int:\n",
    "        numList = [i for i in range(65, 91)]\n",
    "        num = 0\n",
    "\n",
    "        k = 0\n",
    "        for i in columnTitle[::-1]:\n",
    "            num += (ord(i)-64) * (26**k)\n",
    "            k += 1\n",
    "\n",
    "        return num"
   ]
