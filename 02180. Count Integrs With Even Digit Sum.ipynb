   "source": [
    "class Solution:\n",
    "    def countEven(self, num: int) -> int:\n",
    "        count = 0\n",
    "        for i in range(1, num+1):\n",
    "            temp = 0\n",
    "            for j in str(i):\n",
    "                temp += int(j)\n",
    "            if temp%2==0:\n",
    "                count += 1\n",
    "        return count"
   ]
  }
