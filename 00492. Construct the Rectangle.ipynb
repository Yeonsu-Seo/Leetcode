   "source": [
    "class Solution:\n",
    "    def constructRectangle(self, area: int) -> List[int]:\n",
    "        import math\n",
    "\n",
    "        answer = []\n",
    "        \n",
    "        for i in range(1, int(math.sqrt(area))+1):\n",
    "            if area%i==0:\n",
    "                answer.append([area//i,i])\n",
    "        \n",
    "        return answer[-1]"
   ]
  }
