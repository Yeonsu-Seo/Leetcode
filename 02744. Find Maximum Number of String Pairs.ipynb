   "source": [
    "class Solution:\n",
    "    def maximumNumberOfStringPairs(self, words: List[str]) -> int:\n",
    "        result = 0\n",
    "        for i in words:\n",
    "            if i[::-1] in words and i[0] != i[1]:\n",
    "                result += 1\n",
    "        return result//2"
   ]
  }
