   "source": [
    "class Solution:\n",
    "    def largestAltitude(self, gain: List[int]) -> int:\n",
    "        result = [0]\n",
    "        for i in range(len(gain)):\n",
    "            result.append(result[-1]+gain[i])\n",
    "        return max(result)"
   ]
