   "source": [
    "class Solution:\n",
    "    def strStr(self, haystack: str, needle: str) -> int:\n",
    "        if needle in haystack:\n",
    "            haystack = haystack.replace(needle, \"*\")\n",
    "            index = list(haystack).index(\"*\")\n",
    "            return index\n",
    "        else:\n",
    "            return -1"
   ]
