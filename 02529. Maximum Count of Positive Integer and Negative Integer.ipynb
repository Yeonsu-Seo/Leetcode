   "source": [
    "class Solution:\n",
    "    def maximumCount(self, nums: List[int]) -> int:\n",
    "        pos = [i for i in nums if i>0]\n",
    "        return max(len(pos),len(nums)-len(pos)-nums.count(0))"
   ]
  }
