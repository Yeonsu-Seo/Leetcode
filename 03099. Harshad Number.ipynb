   "source": [
    "class Solution:\n",
    "    def sumOfTheDigitsOfHarshadNumber(self, x: int) -> int:\n",
    "        sumofx = 0\n",
    "        for i in str(x):\n",
    "            sumofx += int(i)\n",
    "        if x%sumofx==0:\n",
    "            return sumofx\n",
    "        else:\n",
    "            return -1"
   ]
  }
