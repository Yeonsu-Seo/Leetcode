   "source": [
    "class Solution:\n",
    "    def firstPalindrome(self, words: List[str]) -> str:\n",
    "        for i in words:\n",
    "            for j in range(len(i)//2):\n",
    "                if i[j] != i[len(i)-j-1]:\n",
    "                    break\n",
    "            else:\n",
    "                return i\n",
    "        return \"\""
   ]
  }
