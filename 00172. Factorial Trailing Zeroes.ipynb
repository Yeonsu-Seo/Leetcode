   "source": [
    "class Solution:\n",
    "    def trailingZeroes(self, n: int) -> int:\n",
    "        if n == 0 :\n",
    "            return 0\n",
    "\n",
    "        count = 0\n",
    "        for i in range(1, n+1):\n",
    "            while i%5 == 0:\n",
    "                count += 1\n",
    "                i //= 5\n",
    "\n",
    "        return count"
   ]
  }
