    "class Solution:\n",
    "    def rowAndMaximumOnes(self, mat: List[List[int]]) -> List[int]:\n",
    "        count = []\n",
    "        for i in range(len(mat)):\n",
    "            count.append(mat[i].count(1))\n",
    "\n",
    "        maxi = max(count)\n",
    "\n",
    "        return [count.index(maxi) ,maxi]"
   ]
  }
