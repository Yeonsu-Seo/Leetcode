   "source": [
    "class Solution:\n",
    "    def minLength(self, s: str) -> int:\n",
    "        while (\"AB\" in s) or (\"CD\" in s):\n",
    "            s = \"\".join(s.split(\"AB\"))\n",
    "            s = \"\".join(s.split(\"CD\"))\n",
    "        return len(s)"
   ]
  }
