   "source": [
    "class Solution:\n",
    "    def maximumGap(self, nums: List[int]) -> int:\n",
    "        if len(nums) <= 1:\n",
    "            return 0\n",
    "\n",
    "        nums.sort()\n",
    "        maxNum = 0\n",
    "        for i in range(len(nums)-1):\n",
    "            maxNum = max(maxNum, nums[i+1]-nums[i])\n",
    "        return maxNum"
   ]
  }
 ],
