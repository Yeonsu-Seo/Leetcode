   "source": [
    "class Solution:\n",
    "    def repeatedCharacter(self, s: str) -> str:\n",
    "        appear = []\n",
    "        for i in s:\n",
    "            if i not in appear:\n",
    "                appear.append(i)\n",
    "            else:\n",
    "                return i"
   ]
