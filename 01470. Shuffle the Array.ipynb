   "source": [
    "class Solution:\n",
    "    def shuffle(self, nums: List[int], n: int) -> List[int]:\n",
    "        num1 = nums[:n]\n",
    "        num2 = nums[n:]\n",
    "        result = []\n",
    "        for i in range(n):\n",
    "            result += [num1[i], num2[i]]\n",
    "        return result"
   ]
  }
