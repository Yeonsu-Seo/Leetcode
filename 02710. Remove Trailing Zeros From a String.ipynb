   "source": [
    "class Solution:\n",
    "    def removeTrailingZeros(self, num: str) -> str:\n",
    "        index = 0\n",
    "        for i in num[::-1]:\n",
    "            if i == \"0\":\n",
    "                index += 1\n",
    "            else:\n",
    "                break\n",
    "        return num[:len(num)-index]"
   ]
  }
