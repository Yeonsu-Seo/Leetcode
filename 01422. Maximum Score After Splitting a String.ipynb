   "source": [
    "class Solution:\n",
    "    def maxScore(self, s: str) -> int:\n",
    "        score = 0\n",
    "        for i in range(1, len(s)):\n",
    "            left = s[:i]\n",
    "            right = s[i:]\n",
    "            score = max(score, left.count(\"0\")+right.count(\"1\"))\n",
    "        return score"
   ]
  }
