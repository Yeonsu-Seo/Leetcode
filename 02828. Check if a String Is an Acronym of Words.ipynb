   "source": [
    "class Solution:\n",
    "    def isAcronym(self, words: List[str], s: str) -> bool:\n",
    "        if len(s) != len(words):\n",
    "            return False\n",
    "\n",
    "        i = 0\n",
    "        for j in words:\n",
    "            if s[i] != j[0]:\n",
    "                return False\n",
    "            i += 1\n",
    "        return True"
   ]
  }
