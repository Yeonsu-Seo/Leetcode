   "source": [
    "class Solution:\n",
    "    def areOccurrencesEqual(self, s: str) -> bool:\n",
    "        sList = list(s)\n",
    "        occur = sList.count(sList[0])\n",
    "        for i in set(sList):\n",
    "            if sList.count(i) != occur:\n",
    "                return False\n",
    "        return True"
   ]
  }
