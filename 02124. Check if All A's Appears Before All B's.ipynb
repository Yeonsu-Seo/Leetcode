   "source": [
    "class Solution:\n",
    "    def checkString(self, s: str) -> bool:\n",
    "        if \"a\" not in s:\n",
    "            return True\n",
    "        if \"b\" not in s:\n",
    "            return True\n",
    "        \n",
    "        index = s.index(\"b\")\n",
    "        return \"a\" not in s[index:]"
   ]
