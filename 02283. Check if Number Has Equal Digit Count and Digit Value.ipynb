   "source": [
    "class Solution:\n",
    "    def digitCount(self, num: str) -> bool:\n",
    "\n",
    "        numList = list(num)\n",
    "        for i in num:\n",
    "            if int(i) >= len(numList):\n",
    "                return False\n",
    "            if int(numList[int(i)]) != numList.count(i):\n",
    "                return False\n",
    "        return True"
   ]
  }
