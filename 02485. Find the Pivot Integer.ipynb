   "source": [
    "class Solution:\n",
    "    def pivotInteger(self, n: int) -> int:\n",
    "        nums = [i for i in range(1, n+1)]\n",
    "\n",
    "        for i in range(len(nums)):\n",
    "            num1 = sum(nums[i:])\n",
    "            num2 = sum(nums[:i+1])\n",
    "            if num1 == num2:\n",
    "                return nums[i]\n",
    "        return -1"
   ]
  }
