   "source": [
    "class Solution:\n",
    "    def prefixCount(self, words: List[str], pref: str) -> int:\n",
    "        result = 0\n",
    "        for i in words:\n",
    "            if pref == i[:len(pref)]:\n",
    "                result += 1\n",
    "        return result"
   ]
  }
