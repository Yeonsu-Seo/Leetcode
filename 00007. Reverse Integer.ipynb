   "source": [
    "class Solution:\n",
    "    def reverse(self, x: int) -> int:\n",
    "        if x < 0:\n",
    "            xStr = str(x)[1:]\n",
    "            xStr = -int(xStr[::-1])\n",
    "        else:\n",
    "            xStr = int(str(x)[::-1])\n",
    "        if (xStr>=-2**31) and (xStr<=2**31-1):\n",
    "            return xStr \n",
    "        return 0"
   ]
  }
