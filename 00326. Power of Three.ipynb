   "source": [
    "class Solution:\n",
    "    def isPowerOfThree(self, n: int) -> bool:\n",
    "        if n <= 0:\n",
    "            return False\n",
    "        \n",
    "        while n >= 3:\n",
    "            n /= 3\n",
    "        return n == 1"
   ]
