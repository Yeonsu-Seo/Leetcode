   "source": [
    "class Solution:\n",
    "    def isPowerOfTwo(self, n: int) -> bool:\n",
    "        if n == 1:\n",
    "            return True\n",
    "        while n >= 2:\n",
    "            n /= 2\n",
    "        return n == 1"
   ]
