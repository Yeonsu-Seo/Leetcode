   "source": [
    "class Solution:\n",
    "    def maximumWealth(self, accounts: List[List[int]]) -> int:\n",
    "        maxi = 0\n",
    "        for i in range(len(accounts)):\n",
    "            temp = sum(accounts[i])\n",
    "            if temp>maxi:\n",
    "                maxi = temp\n",
    "        return maxi"
   ]
