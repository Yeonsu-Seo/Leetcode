   "source": [
    "class Solution:\n",
    "    def findClosestNumber(self, nums: List[int]) -> int:\n",
    "        absolute = [x if x>0 else -x for x in nums]\n",
    "        m = min(absolute)\n",
    "        if m in nums:\n",
    "            return m\n",
    "        else:\n",
    "            return -m"
   ]
  }
