   "source": [
    "class Solution:\n",
    "    def findMaxK(self, nums: List[int]) -> int:\n",
    "        numList = [num for num in nums if num>0 and -num in nums]\n",
    "        if numList == []:\n",
    "            return -1\n",
    "        return max(numList)"
   ]
