   "source": [
    "class Solution:\n",
    "    def isThree(self, n: int) -> bool:\n",
    "        import math\n",
    "\n",
    "        count = 0\n",
    "        for i in range(2, int(math.sqrt(n)+1)):\n",
    "            if n%i==0:\n",
    "                if n//i!=i:\n",
    "                    return False\n",
    "                count += 1\n",
    "        return count == 1"
   ]
  }
