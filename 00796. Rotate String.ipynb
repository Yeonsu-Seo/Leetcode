   "source": [
    "class Solution:\n",
    "    def rotateString(self, s: str, goal: str) -> bool:\n",
    "        for i in range(len(s)):\n",
    "            shift = s[i:] + s[:i]\n",
    "            if shift == goal:\n",
    "                return True\n",
    "        return False"
   ]
  }
