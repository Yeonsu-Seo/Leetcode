   "source": [
    "class Solution:\n",
    "    def countOdds(self, low: int, high: int) -> int:\n",
    "        if low%2==0:\n",
    "            low += 1\n",
    "        if high%2==0:\n",
    "            high -= 1\n",
    "        \n",
    "        return (high-low)//2 + 1"
   ]
  }
