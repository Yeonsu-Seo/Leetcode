   "source": [
    "class Solution:\n",
    "    def trimMean(self, arr: List[int]) -> float:\n",
    "        numOf5 = int(len(arr)*0.05)\n",
    "        arr.sort()\n",
    "        for i in range(numOf5):\n",
    "            arr.pop(0)\n",
    "            arr.pop(-1)\n",
    "        \n",
    "        return sum(arr)/len(arr)"
   ]
  }
