   "source": [
    "class Solution:\n",
    "    def maxDepth(self, s: str) -> int:\n",
    "        result = 0\n",
    "        check = 0\n",
    "        for i in s:\n",
    "            if i == \"(\":\n",
    "                check += 1\n",
    "            elif i == \")\":\n",
    "                check -= 1\n",
    "            result = max(check, result)\n",
    "        return result"
   ]
  }
