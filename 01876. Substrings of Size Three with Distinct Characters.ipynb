   "source": [
    "class Solution:\n",
    "    def countGoodSubstrings(self, s: str) -> int:\n",
    "        count = 0\n",
    "        for i in range(len(s)-2):\n",
    "            sub = s[i:i+3]\n",
    "            if len(set(list(sub))) == 3:\n",
    "                count += 1\n",
    "        return count"
   ]
  }
