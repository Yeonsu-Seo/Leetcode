   "source": [
    "class Solution:\n",
    "    def mostWordsFound(self, sentences: List[str]) -> int:\n",
    "        result = []\n",
    "        for i in range(len(sentences)):\n",
    "            result.append(len(sentences[i].split()))\n",
    "        return max(result)"
   ]
