   "source": [
    "class Solution:\n",
    "    def sortedSquares(self, nums: List[int]) -> List[int]:\n",
    "        squares = []\n",
    "        for i in nums:\n",
    "            squares.append(i**2)\n",
    "        squares.sort()\n",
    "        return squares"
   ]
