   "source": [
    "class Solution:\n",
    "    def removeElement(self, nums: List[int], val: int) -> int:\n",
    "        count = 0\n",
    "        for i in range(len(nums)):\n",
    "            if nums[count] == val:\n",
    "                nums.pop(count)\n",
    "                count -= 1\n",
    "            count += 1\n",
    "        return len(nums)"
   ]
  }
